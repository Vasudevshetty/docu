{
  "name": "docu-cli",
  "version": "0.1.0",
  "description": "Blazing-fast, offline-first CLI to fetch, cache, and search developer docs right from your terminal",
  "main": "dist/bin/index.js",
  "bin": {
    "docu": "dist/bin/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/bin/index.js",
    "dev": "ts-node bin/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "cli",
    "documentation",
    "offline",
    "search",
    "typescript"
  ],
  "author": "Vasudevshetty",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "better-sqlite3": "^9.2.2",
    "chalk": "^4.1.2",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/better-sqlite3": "^7.6.8",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
