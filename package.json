{
  "name": "@vasudevshetty/docu-cli",
  "version": "0.3.3",
  "description": "Blazing-fast, AI-powered CLI to fetch, cache, and search developer docs right from your terminal with smart explanations",
  "type": "module",
  "main": "dist/bin/index.js",
  "bin": {
    "docu": "dist/bin/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    ".env.example"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "start": "node dist/bin/index.js",
    "dev": "npm run build && node dist/bin/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vasudevshetty/docu.git"
  },
  "homepage": "https://github.com/Vasudevshetty/docu#readme",
  "bugs": {
    "url": "https://github.com/Vasudevshetty/docu/issues"
  },
  "keywords": [
    "cli",
    "documentation",
    "offline",
    "search",
    "typescript",
    "ai",
    "groq",
    "developer-tools"
  ],
  "author": "Vasudevshetty",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^19.1.8",
    "axios": "^1.6.2",
    "better-sqlite3": "^9.2.2",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.1.0",
    "dotenv": "^16.6.1",
    "groq-sdk": "^0.4.0",
    "ink": "^6.0.1",
    "ink-box": "^2.0.0",
    "ink-divider": "^4.1.1",
    "ink-link": "^4.1.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "ora": "^5.4.1",
    "react": "^19.1.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
